public static void main(String[] args) {
       int num = 12321, result = 0, digit;
       int x=num;
       while(num!=0)
       {
           digit = num%10;
           result = result*10+digit;
           num/=10;
       }
       if(x==result){
            System.out.println("no. is palindrom");}
       else{
            System.out.println("no is not palindrom");}
    }
